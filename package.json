{
	"name": "ts2civet",
	"version": "0.0.0",
	"description": "Converts typescript code to civet code",
	"main": "dist/cli.js",
	"bin": {
		"ts2civet": "dist/cli.js"
	},
	"repository": "gh:esthedebeste/ts2civet",
	"exports": {
		"import": "./dist/transform.js"
	},
	"types": "./dist/transform.d.ts",
	"scripts": {
		"build": "civet ./build.civet",
		"prepare": "civet ./build.civet",
		"start": "civet ./build.civet --dev && node ./dist/cli.js"
	},
	"keywords": [
		"typescript",
		"civet"
	],
	"author": "esthedebeste",
	"license": "MIT",
	"dependencies": {
		"@typescript-eslint/typescript-estree": "^5.59.7",
		"magic-string": "^0.30.0",
		"meow": "^12.0.1",
		"typescript": "^5.0.4"
	},
	"devDependencies": {
		"@danielx/civet": "^0.6.2",
		"@types/node": "^20.2.3",
		"glob": "^10.2.6",
		"prettier": "^2.8.8"
	},
	"files": [
		"dist"
	],
	"type": "module",
	"pnpm": {
		"patchedDependencies": {
			"magic-string@0.30.0": "patches/magic-string@0.30.0.patch"
		}
	}
}
