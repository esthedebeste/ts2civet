{
	"name": "ts2civet",
	"version": "0.1.0",
	"description": "Converts typescript code to civet code",
	"main": "dist/cli.js",
	"bin": {
		"ts2civet": "dist/cli.js"
	},
	"repository": "github:esthedebeste/ts2civet",
	"exports": {
		"import": "./dist/transform.js"
	},
	"types": "./dist/transform.d.ts",
	"scripts": {
		"build": "civet ./build.civet",
		"prepare": "civet ./build.civet",
		"start": "civet ./build.civet --dev && node ./dist/cli.js",
		"verify": "civet ./build.civet --dev && node ./dist/cli.js ./test/test.tsx && civet -c ./test/test.civet -o .verify.tsx",
		"test": "node --loader @danielx/civet/esm --test test/_run.civet"
	},
	"keywords": [
		"typescript",
		"civet"
	],
	"author": "esthedebeste",
	"license": "MIT",
	"dependencies": {
		"code-block-writer": "^12.0.0",
		"glob": "^10.2.6",
		"meow": "^12.0.1",
		"typescript": "^5.1.6"
	},
	"devDependencies": {
		"@danielx/civet": "^0.6.10",
		"@types/node": "^20.2.3",
		"prettier": "^2.8.8"
	},
	"files": [
		"dist/*.js",
		"dist/transform.d.ts"
	],
	"type": "module",
	"pnpm": {
		"patchedDependencies": {
			"code-block-writer@12.0.0": "patches/code-block-writer@12.0.0.patch"
		}
	}
}
